<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>

<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>ccb</title>

	<style>
		input{
			width: 500px;
		}
		
		label{
			width: 140px;
			text-align: right;
			display: inline-block;
			font-weight: bold;
		}
		
		div{
			margin-top: 10px;
		}
		
		.popup{
			width: 100%;
			height: 100%;
			top:0;
			left:0;
			text-align: center;
			margin-top: 20px;
		}
	</style>
</head>
<body onload="document.getElementById('ccbsubmit').submit();">
	<form id="ccbsubmit" name="ccbsubmit" action="https://ibsbjstar.ccb.com.cn/CCBIS/ccbMain" method="post">
		<div class="popup">数据正在加载中......</div>
		<div style="display:none;">
			<div>
				<label>MERCHANTID:</label><input type="text" name="MERCHANTID"
					value="${MERCHANTID }" />
			</div>
			<div>
				<label>POSID:</label><input type="text" name="POSID"
					value="${POSID }" />
			</div>
			<div>
				<label>BRANCHID:</label><input type="text"
					name="BRANCHID" value="${BRANCHID }" />
			</div>
			<div>
				<label>ORDERID:</label><input type="text" name="ORDERID"
					value="${ORDERID }" />
			</div>
			<div>
				<label>PAYMENT:</label><input type="text" name="PAYMENT"
					value="${PAYMENT }" />
			</div>
			<div>
				<label>CURCODE:</label><input type="text" name="CURCODE"
					value="${CURCODE }" />
			</div>
			<div>
				<label>TXCODE:</label><input type="text" name="TXCODE"
					value="${TXCODE }" />
			</div>
			<div>
				<label>REMARK1:</label><input type="text" name="REMARK1"
					value="${REMARK1}" />
			</div>
			<div>
				<label>REMARK2:</label><input type="text" name="REMARK2"
					value="${REMARK2}" />
			</div>
			<div>
				<label>TYPE:</label><input type="text" name="TYPE" value="${TYPE }" />
			</div>
			<div>
				<label>GATEWAY:</label><input type="text" name="GATEWAY"
					value="${GATEWAY }" />
			</div>
			<div>
				<label>CLIENTIP:</label><input type="text"
					name="CLIENTIP" value="${CLIENTIP }" />
			</div>
			<div>
				<label>REGINFO:</label><input type="text"
					name="REGINFO" value="${REGINFO }" />
			</div>
			<div>
				<label>PROINFO:</label><input type="text"
					name="PROINFO" value="${PROINFO }" />
			</div>
			<div>
				<label>REFERER:</label><input type="text"
					name="REFERER" value="${REFERER }" />
			</div>
			<div>
				<label>THIRDAPPINFO:</label><input type="text"
					name="THIRDAPPINFO" value="${THIRDAPPINFO }" />
			</div>
			<div>
				<label>MAC:</label><input type="text"
					name="MAC" value="${MAC }" />
			</div>
			<div>
				<label> </label> <input type="submit" value="提交到建行" />
			</div>
		</div>
	</form>
</body>
</html>